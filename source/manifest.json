{
	"manifest_version": 2,

	"name": "Impequid Bookmarks",
	"description": "Synchronizes your bookmarks with Impequid.",
	"version": "0.0.1",
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions": [
		"bookmarks",
		"*://*/*",
		"storage",
		"alarms"
	]
}
